{
  "version": 2,
  "builds": [
    {
      "src": "api/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.ts"
    }
  ],
  "env": {
    "PORT": "8000",
    "MONGODB_URL": "MONGODB_URL",
    "DB_NAME": "DB_NAME",
    "JWT_SECRET": "JWT_SECRET",
    "NODE_ENV": "NODE_ENV",
    "EMAIL_USER": "EMAIL_USER",
    "EMAIL_PASS": "EMAIL_PASS",
    "FACERECOG_API_URL": "FACERECOG_API_URL",
    "CLIENT_URL": "CLIENT_URL"
  }
}